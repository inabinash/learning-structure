#include <stdio.h>
#include <stdlib.h>
 typedef struct student
{int roll_no;
char name[15];
int classs;
}st;
void print( st st1)
{

    printf("%d %s %d" ,st1.roll_no,st1.name,st1.classs );
}


int main()
{
    st st1;
     printf("Entre the roll ,name,classs repectevily ");
    scanf("%d %s %d",&st1.roll_no,&st1.name,&st1.classs);

    print(st1);


    return 0;
     }


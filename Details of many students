#include <stdio.h>
#include <stdlib.h>
 typedef struct student
{int roll_no;
char name[15];
int classs;
}st;


int main()
{
    int n,i;
    printf("Entre the number of students in the class");
    scanf("%d",&n);

    for(i=0;i<n;i++)
    {
    printf("Entre the roll ,name,classs repectevily ");
    scanf("%d %s %d",&st[i].roll_no,&st[i].name,&st[i].classs);}

     for(i=0;i<n;i++)
     {
    printf("***STODENT DETAILS ARE*****\n");
    printf("%d %s %d",st[i].roll_no,st[i].name,st[i].classs);
    return 0;
     }                                  
}


